@model List<EntityLayer.Concrete.Match>


<div class="fixture-section">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="world-cup">
                    <h1>World Cup</h1>
                </div>
            </div>
        </div>
        <div class="row inner-custom-row">
            <div class="col-md-12">

                @foreach (var item in Model)
                {
                    <div class="row">
                        <div class="white-match">
                            <div class="col-md-4">
                                <div class="match-left">
                                    <div class="fixture-team-img">
                                        <a href="#"><img src="@item.HomeTeam.SignedUrl" width="80" height="77" alt=""></a>
                                    </div>
                                    <div class="fixture-team-name fixture-team-left">
                                        <h1>@item.HomeTeam.Name</h1>
                                        <p>#</p>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="fixture-team-date">
                                    @*<h1>@item.Ho</h1>*@
                                    <p>@item.AwayTeam.Coach</p>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="match-right">
                                    <div class="fixture-team-name fixture-team-right text-right">
                                        <h1>@item.AwayTeam.Name</h1>
                                        <p>#</p>
                                    </div>
                                    <div class="fixture-team-img">
                                        <a href="#"><img src="@item.AwayTeam.SignedUrl" width="64" height="84" alt=""></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                }


            </div>
        </div>
    </div>
</div>